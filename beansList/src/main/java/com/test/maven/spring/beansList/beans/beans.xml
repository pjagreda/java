<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="junglemap"
		class="com.test.maven.spring.beansList.JungleMap">

	<property name="foods">
		<props>
			<prop key="gorilla">banana</prop>
			<prop key="cat">mouse</prop>
			<prop key="snake">eggs</prop>
		</props>
	</property>
	</bean>



	<bean id="lion" class="com.test.maven.spring.beansList.Animal">
		<property name="name" value="Igor"></property>
		<property name="type" value="lion"></property>
	</bean>
	<bean id="elephant"
		class="com.test.maven.spring.beansList.Animal">
		<property name="name" value="Richard"></property>
		<property name="type" value="elephant"></property>
	</bean>
	<bean id="snake" class="com.test.maven.spring.beansList.Animal">
		<property name="name" value="Bob"></property>
		<property name="type" value="snake"></property>
	</bean>
</beans>
